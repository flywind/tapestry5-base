<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
 xmlns:p="tapestry:parameter">
 	<t:if test="cur">
 		<a href="#;" class="${activeCls}" t:type="pagelink" t:page="${page}">
 			<t:if test="item"><i class="${iconCls}"></i></t:if>
 			${value}
 		</a>
 		<p:else>
 			<a href="#;" t:type="pagelink" t:page="${page}">
 				<t:if test="item"><i class="${iconCls}"></i></t:if>
 				${value}
 			</a>
 		</p:else>
 	</t:if>
</t:container>